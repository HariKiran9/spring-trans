CREATE TABLE VEH_DETAILS(VEHICLE_ID NUMBER(20) PRIMARY KEY,
VEHICLE_NAME VARCHAR2(20), CAPACITY NUMBER(20),
MILAGE NUMBER(20), TOTAL_TRIPS NUMBER(20),
DRIVER_ID NUMBER(20) REFERENCES DRV_DETAILS(DRIVER_ID)
ON DELETE CASCADE, HELPER_ID NUMBER(20)
REFERENCES HELP_DETAILS(HELPER_ID) ON DELETE CASCADE);