CREATE TABLE DRV_DETAILS(DRIVER_ID NUMBER(20) PRIMARY KEY,
 DRIVER_NAME VARCHAR2(20), DOB VARCHAR2(20),
  GENDER VARCHAR2(20), QUALIFICATION VARCHAR2(20),
EXPERIENCE VARCHAR2(20), LICENCE_NO VARCHAR2(20),
 LICENCE_TYPE VARCHAR2(20), DOJ VARCHAR2(20), 
 SALARY NUMBER(10,3), LOCT_NAME VARCHAR2(20) 
 REFERENCES LOCATION_DETAILS(LOC_NAME) ON DELETE CASCADE,
 ADDRESS VARCHAR2(20)); 