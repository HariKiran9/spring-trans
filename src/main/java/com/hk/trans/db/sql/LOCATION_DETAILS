CREATE TABLE DRIVER_DETAILS(DRIVER_ID NUMBER(20) PRIMARY KEY, DRIVER_NAME VARCHAR2(20),D0B VARCHAR2(20),GENDER VARCHAR2(20),QUALIFICATION VARCHAR2(20),LICENCE_NO VARCHAR2(20),
LICENCE_TYPE VARCHAR2(20), EXPERIENCE VARCHAR2(20), DOJ VARCHAR2(20), SALARY NUMBER(10,3), LOC_NAME VARCHAR2(20), ADDRESS VARCHAR2(30));
 